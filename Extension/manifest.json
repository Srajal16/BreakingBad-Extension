{
    "manifest_version": 3,
    "name": "Breaking Bad Quotes Extension",
    "version": "1.1",
    "description": "A browser extension that displays Breaking Bad quotes.",
    "icons": {
      "16": "icon.png",
      "48": "icon.png",
      "128": "icon.png"
    },
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "icon.png",
        "48": "icon.png",
        "128": "icon.png"
      }
    },
    "permissions": ["activeTab"]
  }
  